{
  "request": {
    "method": "GET",
    "urlPattern": "\/(|v1\/)user\/deposit_history.*",
    "headers": {
      "ACCESS-KEY": {
        "matches": ".*"
      },
      "ACCESS-REQUEST-TIME": {
        "or": [
          {
            "matches": "[0-9]+"
          },
          {
            "absent": true
          }
        ]
      },
      "ACCESS-TIME-WINDOW": {
        "or": [
          {
            "matches": "^(?:[1-9]|[1-9][0-9]{1,3}|[1-5][0-9]{4}|60000)$"
          },
          {
            "absent": true
          }
        ]
      },
      "ACCESS-NONCE": {
        "or": [
          {
            "matches": "[0-9]+"
          },
          {
            "absent": true
          }
        ]
      },
      "ACCESS-SIGNATURE": {
        "matches": ".*"
      }
    },
    "queryParameters": {
      "asset": {
        "matches": "jpy"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "./private/deposit/deposit_history_jpy.json"
  }
}